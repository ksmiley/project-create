package edu.uky.comm.rcap{	import flash.events.Event;	import edu.uky.comm.rcap.Message;		public class MessageChangeEvent extends Event	{		public var message:Message;		public var textId:String;				public function MessageChangeEvent(			type:String, bubbles:Boolean = false, cancelable:Boolean = false,			inTextId:String = "", inMessage:Message = null		)		{			textId = inTextId;			message = inMessage;			super(type, bubbles, cancelable);		}	}}